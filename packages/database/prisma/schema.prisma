datasource db {
  provider             = "postgresql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
  output          = "../generated/prisma-client"
}

model Task {
  id            String    @id @default(cuid())
  name          String
  description   String?
}
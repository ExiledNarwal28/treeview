{
  "name": "treeview",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --quiet \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:fix": "prettier --quiet --write \"**/*.{js,jsx,ts,tsx,md}\""
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.7.1",
    "turbo": "^1.5.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.5"
}